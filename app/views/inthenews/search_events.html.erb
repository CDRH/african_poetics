<h1 class="sectiontitle"><%= t "inthenews.title" %></h1>

<h2>Events Search</h2>

<%= will_paginate @items %>
<%= pluralize @items.count, "Event" %>

<ul>
  <% @items.each do |item| %>
    <li>
      <%= link_to item.name, inthenews_event_path(item) %>
      | <%= item.event_type %>
    </li>
  <% end %>
</ul>

<%= will_paginate @items %>
