<h1 class="sectiontitle"><%= t "inthenews.title" %></h1>

<h2>Poet Search</h2>

<%= will_paginate @items %>

<ul>
  <% @items.each do |item| %>
    <li>
      <%= link_to item.name, inthenews_poet_path(item) %>
      | <%= item.nationalities %>
    </li>
  <% end %>
</ul>

<%= will_paginate @items %>
