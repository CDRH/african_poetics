<% if @section.include?("inthenews") %>
  <%= render_overridable "partials", "inthenews_title" %>
<% end %>

<% if @section.include?("commentaries") %>
  <h1>Commentaries</h1>
<% elsif params["q"].blank? && params["f"].blank? %>
  <h1><%= t "search.all_items", default: "All Items" %></h1>
<% else %>
  <h1><%= t "search.search_results", default: "Search Results" %></h1>
<% end %>
